class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int>ans;
        vector<int>arr = nums;
        for(int i = 0; i < arr.size(); i++)
        {
            int val = abs(arr[i]);
            
            if(arr[val-1] < 0) ans.push_back(val);
            else
            {
                arr[val-1] *= -1;
            }
        }
        
        return ans;
    }
};
